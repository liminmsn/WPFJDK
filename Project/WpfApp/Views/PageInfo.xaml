<Page x:Class="JDKManage.Views.PageInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:JDKManage.Views"
      mc:Ignorable="d"
      Foreground="White"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageInfo">
	<Grid>
		<StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
			<StackPanel>
				<TextBlock Text="版本信息" FontSize="12" Margin="2"/>
				<Border  Background="Black" CornerRadius="4" VerticalAlignment="Center">
					<TextBlock x:Name="exec" Foreground="YellowGreen" Padding="6" FontSize="12" VerticalAlignment="Top"/>
				</Border>
				<TextBlock Text="途径" FontSize="12" Margin="2"/>
				<Border  Background="Black" CornerRadius="4">
					<StackPanel Orientation="Horizontal" VerticalAlignment="Top">
						<TextBlock x:Name="exec_" Foreground="YellowGreen" Padding="6" FontSize="12"/>
					</StackPanel>
				</Border>
			</StackPanel>
			<!--<Button Margin="0,2,0,0" Click="Button_Click" HorizontalAlignment="Center">检查</Button>-->
		</StackPanel>
	</Grid>
</Page>